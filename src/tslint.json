{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            [
                "element",
                "attribute"
            ],
            [
                "app",
                "record",
                "records",
                "currency",
                "type",
                "admin"
            ],
            "kebab-case"
        ]
    }
}